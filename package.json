{
  "name": "@vmpl/module-bug-replay",
  "version": "1.0.0-dev",
  "description": "Bug Replay module",
  "scripts": {
    "test:static": "tslint --project tsconfig.json",
    "test:static:log": "set -o pipefail; npm run test:static | tee tslint-errors.txt",
    "ts:errors": "tsc --noEmit",
    "test:errors:log": "set -o pipefail; npm run ts:errors | tee tsc-errors.txt",
    "ts:errors:watch": "npm run ts:errors -- -watch",
    "ts:lint": "tslint --fix --project .",
    "ts:defs": "node ./dev/dts-generator.js",
    "ts:build": "babel view/frontend/web/ts/js/ --out-dir view/frontend/web/js/ --extensions '.ts,.tsx' --source-maps",
    "webpack:build": "webpack",
    "cp:rrweb": "rm -rf view/base/web/rrweb && cp -R node_modules/rrweb/dist view/base/web/rrweb",
    "cp:serializer": "rm -rf view/base/web/js/lib/typescript-json-serializer.min.js && cp -R node_modules/typescript-json-serializer/dist/index.cjs.js view/base/web/js/lib/typescript-json-serializer.min.js",
    "ts:watch": "npm run ts:build -- --watch",
    "stylelint": "stylelint '**/*.css' '**/*.less'",
    "start": "concurrently -n 'compiler,errors' -c 'green,red' 'npm run ts:watch' 'npm run ts:errors:watch'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vmpl/magento-bug-replay.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-modules-amd": "^7.20.11",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@babel/types": "^7.21.5",
    "@comandeer/babel-plugin-banner": "^5.0.0",
    "@types/csso": "^5.0.0",
    "@types/jquery": "^3.5.16",
    "@types/jqueryui": "~1.12.16",
    "@types/knockout": "~3.4.71",
    "@types/magento2": "git+https://github.com/magento/ts-types.git",
    "@types/node": "^20.1.0",
    "@types/object-path": "^0.11.1",
    "@types/requirejs": "^2.1.34",
    "@types/slick-carousel": "^1.6.37",
    "@types/tinycolor2": "^1.4.3",
    "@types/tinymce": "^4.6.5",
    "@types/underscore": "^1.11.4",
    "babel-loader": "^9.1.2",
    "concurrently": "^8.0.1",
    "core-js": "^3.30.1",
    "dts-generator": "^3.0.0",
    "eslint": "^8.39.0",
    "glob": "^10.2.2",
    "html2canvas": "^1.4.1",
    "prettier": "^2.8.8",
    "stylelint": "^15.6.1",
    "stylelint-config-recommended": "^12.0.0",
    "stylelint-order": "^6.0.3",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "typescript-json-serializer": "^6.0.0",
    "uuid": "^9.0.0",
    "webpack": "^5.82.0",
    "webpack-cli": "^5.0.2"
  },
  "dependencies": {
    "dexie": "^3.2.3",
    "reflect-metadata": "^0.1.13",
    "rrweb": "^1.1.3",
    "threads": "^1.7.0"
  },
  "engines": {
    "node": "^18.16.0",
    "npm": "^9.5.1"
  },
  "engineStrict": true
}
